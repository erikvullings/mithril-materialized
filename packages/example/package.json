{
  "name": "example",
  "private": true,
  "version": "0.1.4",
  "description": "Example project voor the mithril-materialized library.",
  "scripts": {
    "clean": "rimraf ./.cache",
    "start": "npm run dev",
    "dev": "parcel src/index.html --dist-dir ../../docs",
    "build:domain": "npm run clean && parcel build src/index.html --dist-dir ../../docs --public-url https://erikvullings.github.io/mithril-materialized",
    "link:old": "pnpm link mithril-materialized",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "example",
    "mithril",
    "typescript",
    "materialize-css"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.4",
    "mithril-materialized": "^0.19.0",
    "tslib": "^2.2.0"
  },
  "alias": {
    "mithril": "./node_modules/mithril",
    "materialize-css": "./node_modules/materialize-css"
  },
  "devDependencies": {
    "@parcel/transformer-image": "2.0.0-beta.2",
    "@types/materialize-css": "^1.0.9",
    "@types/mithril": "^2.0.7",
    "autoprefixer": "^10.2.6",
    "cssnano": "^4.1.11",
    "parcel": "^2.0.0-beta.2",
    "rimraf": "^3.0.2",
    "snowpack": "^3.5.2",
    "typescript": "^4.3.2"
  }
}
